{
	"compilerOptions": {
		// DO NOT EDIT
		"outDir": "./dist",
		"target": "es2022",
		"module": "Node16",
		"sourceMap": true,
		// Set your preferences here for TypeScript
		"strict": true,
		"jsx": "react",
		"moduleResolution": "Node16",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": false
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "src/**/*.test.ts"]
}
