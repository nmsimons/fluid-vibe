{
	"name": "fluid-framework-demo",
	"version": "0.0.1",
	"private": "true",
	"description": "",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "npm run compile && npm run webpack",
		"build:prod": "npm run compile && npm run webpack",
		"ci:test": "npm run test",
		"compile": "tsc -b",
		"dev": "cross-env vite serve",
		"dev:network": "cross-env vite serve --host",
		"dev:local": "cross-env VITE_FLUID_CLIENT='local' vite serve",
		"dev:azure": "cross-env VITE_FLUID_CLIENT='azure' vite serve",
		"dev:ios": "cross-env vite serve --host && node scripts/get-network-ip.js",
		"network-ip": "node scripts/get-network-ip.js",
		"format": "prettier src --write",
		"lint": "eslint src",
		"start": "npm run dev",
		"start:server": "azure-local-service",
		"pretest": "playwright install --with-deps",
		"test": "playwright test",
		"test:unit": "vitest run",
		"webpack": "vite build"
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"dependencies": {
		"@azure/msal-browser": "^3.28.0",
		"@fluidframework/azure-client": "^2.72.0",
		"@fluidframework/presence": "^2.72.0",
		"@fluidframework/protocol-definitions": "^3.2.0",
		"@fluidframework/tree": "^2.72.0",
		"@tailwindcss/vite": "^4.0.9",
		"@vitejs/plugin-react": "^4.3.4",
		"axios": "^1.7.9",
		"dotenv": "^16.4.7",
		"fluid-framework": "^2.72.0",
		"jsrsasign": "^11.1.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"unique-names-generator": "^4.7.1",
		"uuid": "^11.0.5",
		"vite": "^7.0.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@fluidframework/azure-local-service": "^2.73.0",
		"@playwright/test": "^1.57.0",
		"@types/jsrsasign": "^10.5.15",
		"@types/react": "^18.3.23",
		"@types/react-dom": "^18.3.7",
		"@typescript-eslint/eslint-plugin": "^8.19.1",
		"@typescript-eslint/parser": "^8.19.1",
		"cross-env": "^7.0.3",
		"eruda": "^3.4.3",
		"eslint": "^9.18.0",
		"eslint-plugin-react": "^7.37.4",
		"globals": "^15.13.0",
		"playwright": "^1.49.0",
		"prettier": "^3.4.2",
		"tailwindcss": "^4.0.9",
		"typescript": "~5.4",
		"vitest": "^4.0.15"
	}
}
